{
  "name": "chrome-extension-teamwork-time-tracker",
  "version": "0.1.0",
  "description": "Chrome extension for Teamwork time tracker",
  "main": "index.js",
  "scripts": {
    "start": "10up-toolkit build -f=umd --watch --external=none --wp=false",
    "build": "10up-toolkit build -f=umd --external=none --wp=false",
    "format-js": "10up-toolkit format-js",
    "lint-js": "10up-toolkit lint-js",
    "lint-style": "10up-toolkit lint-style"
  },
  "keywords": [
    "browser-extesion",
    "teamwork",
    "tenup",
    "time-tracking"
  ],
  "author": "10up",
  "license": "MIT",
  "devDependencies": {
    "10up-toolkit": "^6.3.0",
    "copy-webpack-plugin": "^12.0.2"
  },
  "10up-toolkit": {
    "entry": {
      "content-script": "./src/js/content-script.js",
      "popup": "./src/js/popup.js"
    },
    "useBlockAssets": false
  },
  "dependencies": {
    "dayjs": "^1.11.13",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-modal": "^3.16.1"
  }
}
